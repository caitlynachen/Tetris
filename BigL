# Tetris
package jrJava.Tetris;

public class BigL extends TetrisObject {

	public void createTetrisObject(){
		blocks.add(new Block(-2,0, color));
		blocks.add(new Block(-1,0, color));
		blocks.add(new Block(0,0, color));
		blocks.add(new Block(0,1, color));
		blocks.add(new Block(0,2, color));
	}
}
